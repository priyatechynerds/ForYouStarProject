<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.priya.foryoustarproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/star_icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/star_icon1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationDrawer"
            android:label="@string/title_activity_navigation_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CompanyNextPage" />
        <activity android:name=".AboutCompany" />
        <activity android:name=".Support" />
        <activity android:name=".Downloads" />
        <activity android:name=".Legals" />
        <activity android:name=".Certificates" />
        <activity android:name=".Slider" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".PDFViewer" />
        <activity android:name=".BankDetails" />
        <activity android:name=".Website" />

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".Login" />
        <activity android:name=".JoiningForm"></activity>
    </application>

</manifest>